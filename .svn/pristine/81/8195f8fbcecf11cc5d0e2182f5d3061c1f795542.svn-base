package edu.uestc.tc.beans;

import java.util.ArrayList;
import java.util.Date;

public class CommiteGrid {
	private ArrayList<UnitGrid> unitList = new ArrayList<UnitGrid>();
	Date curruntDate = null;

	public void clear() {
		this.unitList.clear();
		curruntDate = null;
	}

	public Date getCurruntDate() {
		return curruntDate;
	}

	public ArrayList<UnitGrid> getUnitList() {
		return unitList;
	}

	public void push(UnitGrid unitGrid) {
		this.unitList.add(unitGrid);
	}

	public void setCurruntDate(Date curruntDate) {
		this.curruntDate = curruntDate;
	}

	public void setUnitList(ArrayList<UnitGrid> unitList) {
		this.unitList = unitList;
	}
}
